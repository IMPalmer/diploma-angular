{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*" :
      ["src/app/components/*"],
      "@services/*" :
      ["src/app/shared/services/*"],
      "@models/*" :
      ["src/app/shared/models/*"],
      "@directives/*" :
      ["src/app/shared/directives/*"],
      "@guards/*" :
      ["src/app/shared/guards/*"],
      "@interceptors/*" :
      ["src/app/shared/interceptors/*"]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false
  }
}
